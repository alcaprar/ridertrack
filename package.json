{
  "name": "ridertrack-web",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test-frontend": "ng test",
    "test-backend": "cross-env DATABASE_NAME=test mocha test --exit",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --prod",
    "dev-backend": "nodemon server.js",
    "dev-frontend": "ng server --proxy-config ./proxy.config.json",
    "dev": "concurrently --kill-others \"npm run dev-backend\" \"npm run dev-frontend\"",
    "prod": "ng build --prod && npm start"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "cacheDirectories": [
    "node_modules",
    "swagger",
    "scripts"
  ],
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "e2e/*",
      "node_modules/*",
      "src/*",
      "test/*"
    ]
  },
  "private": true,
